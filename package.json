{
  "name": "yeet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "pregraphql:schema": "rimraf src/lib/graphql-schema.json",
    "graphql:schema": "apollo client:download-schema --endpoint=http://localhost:3000/graphql src/lib/graphql-schema.json",
    "pregraphql:types": "yarn graphql:schema && rimraf src/lib/graphql && mkdir src/lib/graphql",
    "graphql:types": "apollo client:codegen src/lib/graphql --config=apollo.config.js --localSchemaFile=src/lib/graphql-schema.json  --includes=\"src/lib/*.graphql\" --target typescript --passthroughCustomScalars --outputFlat --namespace GraphQL",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
